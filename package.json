{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "storybook-search",
  "title": "Storybook Search",
  "description": "Search components in Storybook",
  "icon": "command-icon.png",
  "author": "nakaakist",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Components",
      "description": "Search Storybook components",
      "mode": "view"
    },
    {
      "name": "edit-config",
      "title": "Edit Config",
      "description": "Edit config in Search Storybook components",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.3",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
